<div class="row">
                            <div class="col-sm-12">
                                <div class="card-box table-responsive">
                                  

                              <h4 class="header-title m-t-0 m-b-30">Gerenciamento de Comitês</h4>

                                    <table id="datatable-buttons" class="table table-striped table-bordered">
                                        <thead>
                                            <tr>
                                                <th>Nome</th>
                                                <th>Data Início</th>
                                                <th>Data Final</th>
                                                <th>Total não cotista</th>
                                                <th>Total cotista</th>
                                                <th>Preço</th>
                                                <th>Preço por Cotas</th>
                                                <th>Opções</th>
                                            </tr>
                                        </thead>

                                        <tbody>
                                        <% @comitees.each do |comitee| %>
                                            <tr>
                                                <td><%=comitee.name%></td>
                                                <td><%=comitee.start_date%></td>
                                                <td><%=comitee.end_date%></td>
                                                <td><%=comitee.users.where(is_cotist:false).count %>/<%=comitee.limit%></td>
                                                <td><%=comitee.users.where(is_cotist:true).count %>/<%=comitee.limit_cotist%></td>
                                                <td><%=comitee.value_cotist%></td>
                                                <td><%=comitee.value_not_cotist%></td>
                                                <td><%= link_to  edit_crew_comitee_path(comitee) do %><button class="btn btn-icon waves-effect waves-light btn-warning m-b-5"> <i class="fa fa-wrench"></i> </button> <% end %>
                                                <%= link_to  crew_comitee_path(comitee) do %>
                                                <button class="btn btn-icon waves-effect waves-light btn-primary m-b-5"> <i class="fa fa-users"></i>  </button><% end %>
                                                <button class="btn btn-icon waves-effect waves-light btn-danger m-b-5 __web-inspector-hide-shortcut__"> <i class="fa fa-remove"></i></button></td>
                                            </tr>
                                            <% end %>
                                        </tbody>
                                    </table>
                                </div>
                            </div><!-- end col -->
                        </div>
                        <!-- end row -->


