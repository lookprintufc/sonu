<div class="row">
   <div class="col-sm-12">
      <div class="card-box table-responsive">

      <%=link_to new_crew_commission_path do %><button type="button" class="btn btn-primary waves-effect w-md waves-light m-b-5 pull-right">Adicionar</button><% end %>
         <h4 class="header-title m-t-0 m-b-30">Gerenciamento da página Comitê</h4>
         <table id="datatable-buttons" class="table table-striped table-bordered">
            <thead>
               <tr>
                  <th>Título </th>
                  <th>-</th>
               </tr>
            </thead>
            <tbody>
               <% @commissions.each do |com| %>
               <tr>
                  <td><%=com.title%></td>
                  <td><%= link_to  new_crew_commission_path(@commission) do %><button data-toggle="tooltip" data-placement="left" title="Editar comitê" class="btn btn-icon waves-effect waves-light btn-warning m-b-5"> <i class="fa fa-wrench"></i> </button> <% end %></td>
                  
               </tr>
               <% end %>
            </tbody>
         </table>
      </div>
   </div>
   <!-- end col -->
</div>
<!-- end row -->