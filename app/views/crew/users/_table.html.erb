<!-- Main content -->
<section class="content">
   <!-- <section class="box"> -->
   <div class="row">
      <div class="col-xs-12">
         <div class="box">
            <div class="box-header">
               <h3 class="box-title">Gerenciamento de Usuários</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
               <table id="table_ecej" class="table table-striped table-bordered dt-responsive nowrap display"  cellspacing="0" width="100%">
                  <thead>
                     <tr>
                        <th>Nome</th>
                        <th>Email</th>
                        <th class="noExport">Inscrito em Evento</th>
                        <th hidden="">Cotista</th>
                        <th>Data de Inscr.</th>
                        <th class="noExport"></th>
                        <th class="noExport"></th>
                        <th class="noExport"></th>
                     </tr>
                  </thead>
                  <tbody>
                     <% j=1 %>
                     <% @users.each do |users| %>
                     <tr>
                        <td><%= users.name.nil? ? 'Não Informado' : users.name.titleize%></td>
                        <td><%= users.email%></td>
                        <td>

                        </td>
                        <td>
                        </td>

                        </td><td> <%= link_to edit_crew_user_path(users), target: "_blank", method: :patch do %><button type="button" class="btn btn-block btn btn-sm">EDITAR</button> <% end %>
                        <td> <%= link_to crew_user_path(users), target: "_blank", method: :patch do %><button type="button" class="btn btn-block btn-warning btn-sm">VER</button> <% end %>
                        <td> <%= link_to crew_user_path(users), target: "_blank", method: :patch do %><button type="button" class="btn btn-block btn-danger btn-sm">DELETAR</button> <% end %>
                      </td>
                     </tr>
                     <% end %>
                  </tbody>
               </table>
            </div>
         </div>
      </div>
   </div>
   <!-- </section> -->
</section>
