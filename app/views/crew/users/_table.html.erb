<!-- Main content -->
<section class="content">
    <!-- <section class="box"> -->
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">Gerenciamento de Usuários</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="table_ecej" class="table table-striped table-bordered dt-responsive nowrap display" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>Nome</th>
                                <th>Email</th>
                                <th class="noExport">Inscrito</th>
                                <th hidden="">Cotista</th>
                                <th>Data de Inscr.</th>
                                <th class="noExport"></th>
                                <th class="noExport"></th>
                                <th class="noExport"></th>
                            </tr>
                        </thead>
                        <tbody>
                            <% j=1 %>
                            <% @users.each do |user| %>
                            <tr>
                                <td><%= user.name.nil? ? 'Não Informado' : user.name.titleize%></td>
                                <td><%= user.email%></td>
                                <td></td>

                                <td><%= user.created_at.strftime("%d-%m-%Y  %H:%M:%S") %></td>
                                <td>
                                    <%= link_to edit_crew_user_path(user), target: "_blank", method: :patch do %>
                                    <button type="button" class="btn btn-block btn-sm">Editar</button>
                                    <% end %>
                                    <%#= link_to users, method: :delete, data: { confirm: 'Are you sure?' } do %>
                                    <a href="">

                                        <!-- <span class="glyphicon glyphicon-remove" aria-hidden="true" style="margin-left: 20px"></span> -->
                                        <%# end %>
                                    </td>
                                    <td>
                                        <%= link_to crew_user_path(user), target: "_blank", method: :patch do %>
                                        <button type="button" class="btn btn-block btn-warning btn-sm">Ver</button>
                                        <% end %>
                                    </td>
                                    <td>
                                        <%= link_to crew_user_path(user), target: "_blank", method: :patch do %>
                                        <button type="button" class="btn btn-block btn-danger btn-sm">Deletar</button>
                                        <% end %></td>
                                </tr>
                                <% end %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <!-- </section> -->
    </section>
